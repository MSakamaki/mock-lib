    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsoneditor": "^5.14.1",
    "prettier": "^1.12.1",
    "style-loader": "^0.21.0",
    "svg-url-loader": "^2.3.2",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
